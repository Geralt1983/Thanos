# Example environment configuration
# Copy this file to .env and fill in your values

# API Authentication (required for production)
TASK_API_KEY=your-api-key-here
TASK_API_URL=https://api.example.com

# Server Configuration
DEBUG=false                # Enable debug logging
ALLOW_WRITE=false          # Enable create/update/delete operations
NODE_ENV=production        # Environment: development, test, production
