{
  "litellm": {
    "providers": {
      "anthropic": {
        "api_key_env": "ANTHROPIC_API_KEY",
        "models": {
          "opus": "claude-3-opus-20240229",
          "sonnet": "claude-3-5-sonnet-20241022",
          "haiku": "claude-3-5-haiku-20241022"
        }
      },
      "openai": {
        "api_key_env": "OPENAI_API_KEY",
        "models": {
          "gpt41_nano": "gpt-4.1-nano",
          "gpt41_mini": "gpt-4.1-mini",
          "gpt4o": "gpt-4o",
          "gpt4": "gpt-4-turbo-preview",
          "gpt35": "gpt-3.5-turbo"
        }
      },
      "google": {
        "api_key_env": "GOOGLE_API_KEY",
        "models": {
          "gemini_pro": "gemini/gemini-pro",
          "gemini_flash": "gemini/gemini-1.5-flash"
        }
      }
    },
    "default_provider": "openai",
    "default_model": "gpt-4.1-nano",
    "allow_fallback_client": true,
    "fallback_chain": [
      "gpt-4.1-nano",
      "gpt-4.1-mini",
      "gpt-4o"
    ],
    "timeout": 600,
    "max_retries": 3,
    "retry_delay": 1.0
  },
  "agent_routing": {
    "enabled": true,
    "agents": {
      "ops": {
        "model": "claude-3-5-haiku-20241022",
        "fallback": "gpt-4.1-mini",
        "description": "Fast operations, tool calls, scheduling"
      },
      "coach": {
        "model": "claude-3-5-haiku-20241022",
        "fallback": "gpt-4.1-mini",
        "description": "Empathetic coaching, accountability"
      },
      "strategy": {
        "model": "claude-3-5-sonnet-20241022",
        "fallback": "claude-3-5-sonnet-20241022",
        "description": "Complex reasoning, big-picture planning"
      },
      "health": {
        "model": "claude-3-5-haiku-20241022",
        "fallback": "gpt-4.1-mini",
        "description": "Sensitive health topics, ADHD support"
      }
    },
    "default_agent_model": "gpt-4.1-nano"
  },
  "model_routing": {
    "rules": {
      "complex": {
        "model": "claude-3-5-sonnet-20241022",
        "indicators": [
          "architecture",
          "analysis",
          "strategy",
          "planning",
          "debug",
          "refactor"
        ],
        "min_complexity": 0.7
      },
      "standard": {
        "model": "claude-3-5-haiku-20241022",
        "indicators": [
          "task",
          "summary",
          "format",
          "quick",
          "explain"
        ],
        "min_complexity": 0.3
      },
      "simple": {
        "model": "gpt-4.1-nano",
        "indicators": [
          "lookup",
          "translate",
          "format",
          "simple",
          "hello"
        ],
        "max_complexity": 0.3
      }
    },
    "complexity_factors": {
      "token_count_weight": 0.3,
      "keyword_weight": 0.4,
      "history_length_weight": 0.3
    },
    "default_complexity": 0.5
  },
  "usage_tracking": {
    "enabled": true,
    "storage_path": "State/usage.json",
    "alert_threshold": 100000,
    "cost_projection_enabled": true,
    "pricing": {
      "claude-3-opus-20240229": {
        "input": 0.015,
        "output": 0.075
      },
      "claude-3-5-sonnet-20241022": {
        "input": 0.003,
        "output": 0.015
      },
      "claude-3-5-haiku-20241022": {
        "input": 0.00025,
        "output": 0.00125
      },
      "gpt-4o": {
        "input": 0.005,
        "output": 0.015
      },
      "gpt-4-turbo-preview": {
        "input": 0.01,
        "output": 0.03
      },
      "gpt-4.1-nano": {
        "input": 0.0002,
        "output": 0.0008
      },
      "gpt-4.1-mini": {
        "input": 0.001,
        "output": 0.004
      },
      "gpt-3.5-turbo": {
        "input": 0.0005,
        "output": 0.0015
      },
      "gemini/gemini-pro": {
        "input": 0.00025,
        "output": 0.0005
      },
      "gemini/gemini-1.5-flash": {
        "input": 0.000075,
        "output": 0.0003
      }
    }
  },
  "caching": {
    "enabled": true,
    "ttl_seconds": 3600,
    "storage_path": "Memory/cache/",
    "max_cache_size_mb": 100,
    "cache_by_model": true
  },
  "defaults": {
    "max_tokens": 4096,
    "temperature": 1.0,
    "stream": false
  },
  "interactive_prompt": {
    "enabled": true,
    "mode": "compact",
    "show_duration": true,
    "show_message_count": false,
    "color_coding": {
      "enabled": true,
      "thresholds": {
        "low": 0.50,
        "medium": 2.00
      }
    }
  },
  "openai_responses": {
    "enabled": false,
    "model_prefixes": [
      "gpt-"
    ],
    "tiers": {
      "simple": {
        "verbosity": "low",
        "reasoning_effort": "low",
        "max_output_tokens": 512
      },
      "standard": {
        "verbosity": "medium",
        "reasoning_effort": "medium",
        "max_output_tokens": 2048
      },
      "complex": {
        "verbosity": "high",
        "reasoning_effort": "high",
        "max_output_tokens": 4096
      }
    }
  },
  "startup": {
    "default_command": "/pa:daily"
  },
  "mcp_servers": {
    "workos": {
      "command": "npx",
      "args": ["tsx", "mcp-servers/workos-mcp/src/index.ts"],
      "transport": "stdio",
      "enabled": true,
      "description": "WorkOS MCP server for task management, habits, energy tracking, and brain dumps"
    },
    "gsuite": {
      "command": "uvx",
      "args": ["mcp-gsuite"],
      "transport": "stdio",
      "enabled": true,
      "cwd": "/Users/jeremy/.config/mcp-gsuite",
      "description": "Google Suite MCP server for Calendar, Gmail, and Drive"
    }
  }
}